{
  "$schema": "https://json.schemastore.org/package",
  "name": "image-process-loader",
  "version": "1.1.1",
  "description": "Image process loader for webpack",
  "homepage": "https://github.com/foray1010/image-process-loader",
  "bugs": "https://github.com/foray1010/image-process-loader/issues",
  "repository": "foray1010/image-process-loader",
  "license": "MIT",
  "author": "foray1010",
  "exports": "./src/index.js",
  "main": "./src/index.js",
  "files": [
    "src",
    "*.md"
  ],
  "scripts": {
    "ci": "yarn lint:js && yarn lint:md",
    "eslint": "eslint --ext=cjs,cts,js,mjs,mts,ts,tsx --ignore-path=node_modules/@foray1010/eslint-config/eslintignore",
    "lint:js": "yarn eslint .",
    "lint:md": "yarn remark .",
    "prepare": "husky install",
    "prettier": "prettier --ignore-path=node_modules/@foray1010/prettier-config/prettierignore",
    "remark": "remark --frail --ignore-path=node_modules/@foray1010/remark-preset/remarkignore --ignore-path-resolve-from=cwd --silently-ignore"
  },
  "dependencies": {
    "loader-utils": "^2.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.2.0",
    "@commitlint/config-conventional": "17.2.0",
    "@foray1010/eslint-config": "9.0.1",
    "@foray1010/prettier-config": "8.0.0",
    "@foray1010/remark-preset": "8.0.0",
    "eslint": "8.26.0",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "remark-cli": "11.0.0",
    "sharp": "0.31.1"
  },
  "peerDependencies": {
    "sharp": "*"
  },
  "engines": {
    "node": "^14.18.0 || ^16.13.0 || >=18.12.0"
  }
}
