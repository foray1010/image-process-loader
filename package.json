{
  "$schema": "https://json.schemastore.org/package",
  "name": "image-process-loader",
  "version": "1.1.1",
  "description": "Image process loader for webpack",
  "homepage": "https://github.com/foray1010/image-process-loader",
  "bugs": "https://github.com/foray1010/image-process-loader/issues",
  "repository": "foray1010/image-process-loader",
  "license": "MIT",
  "author": "foray1010",
  "exports": "./src/index.js",
  "main": "./src/index.js",
  "files": [
    "src",
    "*.md"
  ],
  "scripts": {
    "ci": "yarn lint:js && yarn lint:md",
    "eslint": "eslint --ext=cjs,cts,js,mjs,mts,ts,tsx --ignore-path=node_modules/@foray1010/eslint-config/eslintignore",
    "postinstall": "husky install",
    "lint:js": "yarn eslint .",
    "lint:md": "yarn remark .",
    "prepublishOnly": "pinst --disable",
    "prettier": "prettier --ignore-path=node_modules/@foray1010/prettier-config/prettierignore",
    "postpublish": "pinst --enable",
    "remark": "remark --frail --ignore-path=node_modules/@foray1010/remark-preset/remarkignore --ignore-path-resolve-from=cwd"
  },
  "dependencies": {
    "loader-utils": "^2.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "13.2.1",
    "@commitlint/config-conventional": "13.2.0",
    "@foray1010/eslint-config": "5.0.4",
    "@foray1010/prettier-config": "5.0.1",
    "@foray1010/remark-preset": "4.0.1",
    "eslint": "7.32.0",
    "husky": "7.0.2",
    "lint-staged": "11.2.6",
    "pinst": "2.1.6",
    "prettier": "2.4.1",
    "remark-cli": "9.0.0",
    "sharp": "*"
  },
  "peerDependencies": {
    "sharp": "*"
  },
  "engines": {
    "node": ">=12.13"
  }
}
